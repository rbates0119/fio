[global]
filename=/dev/nvme1n1
group_reporting=0
ba=4K
bs=4K
iodepth=1
rw=randwrite
disable_clat=1
disable_slat=1
ioengine=libaio
direct=1
zonemode=zbd
issue_zone_finish=1
reset_all_zones_first=1
fill_empty_zones_first=1
zrwa_alloc=1
iodepth_batch_complete_min=4
[zone1]
offset=0
num_zones=10
max_open_zones=1
zrwa_overwrite_percent=2
[zone2]
offset=20G
num_zones=10
max_open_zones=1
zrwa_overwrite_percent=2

