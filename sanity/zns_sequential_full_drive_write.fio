# This INI is used to do a FULL drive write using sequential zones.
# No ZRWA is associated.
# Only one active zone is present at a time.
[seq-write_bs128K_qd256]
name=zc-seq-write
filename=/dev/nvme1n1
rw=write
bs=128k
direct=1
numjobs=1
#size=100%
io_size=646200M
size=1200g
zonemode=zbd
max_open_zones=1
ioengine=libaio
#Sequential zone needs to operate at QD1.
iodepth=1
#cpumask=cpus_allowed
#issue_zone_finish=1
reset_all_zones_first=1

