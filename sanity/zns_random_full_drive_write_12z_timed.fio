# This INI is used to do a FULL drive write using random zones.
# No ZRWA is associated.
# 12 active zone is present at a time.
[rand_timed_12z]
filename=/dev/nvme1n1
rw=randwrite
bs=128k
direct=1
numjobs=1
zonemode=zbd
time_based=1
runtime=5m
max_open_zones=12
ioengine=libaio
iodepth=1
ns_id=1
#reset_all_zones_first=1
fill_empty_zones_first=1
#issue_zone_finish=1

