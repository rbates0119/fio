[global]
ioengine=libaio
direct=1
time_based=1
runtime=10m
[JOB1_random-write_cns]
filename=/dev/nvme1n1
rw=randwrite
bs=4K
iodepth=1
norandommap=1
flow=512
[JOB2_seq-write_1_zone]
filename=/dev/nvme1n2
rw=write
bs=16K
max_open_zones=1
iodepth=8
zonemode=zbd
issue_zone_finish=1
zrwa_alloc=1
ns_id=2
zrwa_overwrite_percent=50
flow=-1
[JOB3_rnd-read_zns]
filename=/dev/nvme1n2
rw=randread
read_beyond_wp=1
bs=16K
iodepth=1
zonemode=zbd
size=2g
